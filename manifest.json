{
  "manifest_version": 3,
  "version": "0.1",
  "name": "new tab, new image",
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "permissions": ["storage", "unlimitedStorage", "contextMenus"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}
